{
  "name": "device_derived_stats_us",
  "owner": "FirmwareVDP",
  "schedules": [
    {
      "name": "daily",
      "cron": "00 16 * * *",
      "backfill_window": 1,
      "backfill_batch_size": 1
    }
  ],
  "container_config": {
    "resources": {
      "requests": {
        "cpu": "4000m",
        "memory": "4096Mi"
      }
    }
  },
  "max_concurrent": 10,
  "assets": [
    "us-west-2:product_analytics.agg_device_stats_secondary_consistency",
    "us-west-2:product_analytics.agg_device_stats_secondary_coverage",
    "us-west-2:product_analytics.agg_device_stats_secondary_response_ratio",
    "us-west-2:product_analytics.agg_device_stats_secondary_ratio"
  ]
}
