{
  "owner": "DataPlatform",
  "jobs": [
    {
      "name": "data streams live ingestion",
      "cron": "57 0/20 * * * ?",
      "timeout_seconds": 7200,
      "regions": ["us-west-2", "eu-west-1", "ca-central-1"],
      "spark_version": "datastreams",
      "driver_node_type_id": "rd-fleet.2xlarge",
      "node_type_id": "rd-fleet.2xlarge",
      "min_workers": 4,
      "max_workers": 8,
      "rnd_allocation": 0,
      "spark_conf_overrides": {
        "spark.databricks.rocksDB.fileManager.compactSmallFilesThreshold": "16"
      },
      "slo_config": {
        "business_hours_threshold_hours": 1,
        "high_urgency_threshold_hours": 3
      }
    }
  ],
  "default_libraries_to_skip": ["bokeh"]
}
