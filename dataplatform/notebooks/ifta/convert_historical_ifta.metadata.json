{
  "owner": "IftaCore",
  "jobs": [
    {
      "name": "convert",
      "regions": ["us-west-2", "eu-west-1", "ca-central-1"],
      "timeout_seconds": 86400,
      "max_workers": 64,
      "spark_version": "default"
    }
  ],
  "output": {
    "dbname": "fuel_maintenance",
    "tablename": "historical_ifta_report_delta",
    "type": "table",
    "schema": [
      {
        "name": "date",
        "type": "date"
      },
      {
        "name": "org_id",
        "type": "long"
      },
      {
        "name": "device_id",
        "type": "long"
      },
      {
        "name": "driver_id",
        "type": "long"
      },
      {
        "name": "jurisdiction",
        "type": "string"
      },
      {
        "name": "end_ms",
        "type": "long"
      },
      {
        "name": "duration_ms",
        "type": "long"
      },
      {
        "name": "gps_distance_m",
        "type": "long"
      },
      {
        "name": "odo_distance_m",
        "type": "long"
      },
      {
        "name": "toll_distance_m",
        "type": "long"
      },
      {
        "name": "time_ms",
        "type": "long"
      },
      {
        "name": "tripless_gps_distance_m",
        "type": "long"
      },
      {
        "name": "tripless_odo_distance_m",
        "type": "long"
      },
      {
        "name": "tripless_toll_distance_m",
        "type": "long"
      },
      {
        "name": "tripless_engine_on_ms",
        "type": "long"
      },
      {
        "name": "use_odometer",
        "type": "boolean"
      },
      {
        "name": "use_tripless",
        "type": "boolean"
      }
    ]
  }
}
