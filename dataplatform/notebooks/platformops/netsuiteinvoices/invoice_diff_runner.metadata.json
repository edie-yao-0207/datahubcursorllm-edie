{
  "owner": "PlatformOperations",
  "email_notifications": ["platops-notebook-aler-aaaad6yplk2cd33xw6iwfq3kxi@samsara.org.slack.com"],
  "jobs": [
    {
      "name": "netsuite-invoice-diff-generation",
      "cron": "0 */10 * * * ? 2099",
      "parameters": {
        "NOTEBOOK_NAME": "invoice",
        "NETSUITE_CHILD_INVOICES_VIEW": "netsuite_data.netsuite_child_invoices",
        "NETSUITE_CONSOLIDATED_INVOICES_VIEW": "netsuite_data.netsuite_consolidated_invoices",
        "CONSOLIDATED_INVOICES_MAP_VIEW": "netsuite_data.consolidated_invoices_map",
        "LATEST_ROW_SYNCED_KEY": "latest_row_synced.timestamp",
        "LATEST_SUCCESSFULLY_UPDATE_KEY": "last_updated.txt",
        "CONSOLIDATED_INVOICE_MAP_CSV_KEY": "ci_map.csv"
      },
      "regions": ["us-west-2", "eu-west-1"],
      "rnd_allocation": 0,
      "spark_version": "default",
      "unity_catalog_setting": {
        "data_security_mode": "USER_ISOLATION"
      },
      "timeout_seconds": 10800
    }
  ]
}
