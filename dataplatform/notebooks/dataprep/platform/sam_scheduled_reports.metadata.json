{
  "owner": "DataEngineering",
  "email_notifications": ["kalyan.vejalla@samsara.com", "steven.matson@samsara.com"],
  "jobs": [
    {
      "name": "update",
      "cron": "0 0 1-23/3 * * ?",
      "regions": ["us-west-2", "eu-west-1"],
      "spark_version": "default"
    }
  ],
  "output": {
    "dbname": "dataprep",
    "tablename": "sam_scheduled_reports",
    "type": "table",
    "schema": [
      {
        "name": "date",
        "type": "date",
        "nullable": true
      },
      {
        "name": "sam_number",
        "type": "string",
        "nullable": true
      },
      {
        "name": "num_scheduled_reports",
        "type": "long",
        "nullable": true
      }
    ],
    "primary_key": ["date", "sam_number"]
  }
}
