      },
      {
        "name": "date",
        "type": "date"
      },
      {
        "name": "delighted_id",
        "type": "long"
      },
      {
        "name": "raw_score",
        "type": "long"
      },
      {
        "name": "nps_score",
        "type": "long"
      },
      {
