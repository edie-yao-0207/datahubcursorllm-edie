{
  "owner": "DataEngineering",
  "email_notifications": ["kalyan.vejalla@samsara.com", "steven.matson@samsara.com"],
  "jobs": [
    {
      "name": "update",
      "cron": "0 0 0/3 * * ?",
      "regions": ["us-west-2", "eu-west-1"],
      "spark_version": "default"
    }
