      },
      {
        "name": "delighted_role",
        "type": "string"
      },
      {
        "name": "locale",
        "type": "string"
      },
      {
        "name": "delighted_account_id",
        "type": "string"
      },
      {
        "name": "delighted_contact_id",
        "type": "string"
      },
      {
        "name": "theme",
        "type": "string"
      },
      {
        "name": "bucket",
