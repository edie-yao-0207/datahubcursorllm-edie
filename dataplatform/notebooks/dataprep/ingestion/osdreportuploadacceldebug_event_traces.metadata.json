{
  "owner": "DataEngineering",
  "description": "This table contains 'cleansed' osdaccelerometer event traces from debug reports. Each row represents the osdaccelerometer trace data from a single debug report request.",
  "email_notifications": ["kalyan.vejalla@samsara.com", "steven.matson@samsara.com"],
  "jobs": [
    {
      "name": "restate-daily",
      "cron": "0 0 9 * * ? *",
      "regions": ["us-west-2"],
      "parameters": {
        "start_date": "",
        "end_date": ""
      },
      "spark_version": "default",
      "driver_node_type_id": "rd-fleet.2xlarge",
      "node_type_id": "rd-fleet.2xlarge",
      "max_workers": 10,
      "spark_conf_overrides": {
        "spark.executor.cores": "2",
        "spark.network.timeout": "500",
        "spark.databricks.adaptive.autoOptimizeShuffle.enabled": "true"
      },
      "timeout_seconds": 28800
    },
    {
      "name": "restate-backfill",
      "regions": ["us-west-2"],
      "parameters": {
        "start_date": "",
        "end_date": ""
      },
      "spark_version": "default",
      "driver_node_type_id": "rd-fleet.2xlarge",
      "node_type_id": "rd-fleet.2xlarge",
      "max_workers": 64,
      "spark_conf_overrides": {
        "spark.executor.cores": "2",
        "spark.network.timeout": "500",
        "spark.databricks.adaptive.autoOptimizeShuffle.enabled": "true"
      },
      "timeout_seconds": 28800
    }
  ]
