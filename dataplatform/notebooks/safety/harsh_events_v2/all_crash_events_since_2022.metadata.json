{
  "owner": "SafetyPlatform",
  "jobs": [
    {
      "name": "update",
      "cron": "0 0 0/3 * * ?",
      "regions": ["us-west-2", "eu-west-1"],
      "timeout_seconds": 7200,
      "spark_version": "default"
    }
  ],
  "output": {
    "dbname": "dataprep_safety",
    "tablename": "all_crash_events_since_2022",
    "type": "table",
    "schema": [
      {
        "name": "date",
        "type": "date"
      },
      {
        "name": "org_id",
        "type": "long"
      },
      {
        "name": "device_id",
        "type": "long"
      },
      {
        "name": "event_ms",
        "type": "long"
      },
      {
        "name": "hidden_to_customer",
        "type": "boolean"
      },
      {
        "name": "trip_start_ms",
        "type": "long"
      },
      {
        "name": "trip_end_ms",
        "type": "long"
      },
      {
        "name": "ms_after_trip_end",
        "type": "long"
      }
    ]
  }
}
