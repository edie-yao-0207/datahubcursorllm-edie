{
  "view": "netsuite_deletion",
  "owner": "PlatformOperations",
  "description": "This view gathers netsuite transactions that should be deleted in finopsDb",
  "database": "netsuite_data",
  "use_regional_values": true,
  "regions": ["us-west-2"],
  "schema": [
    { "name": "transaction_id", "type": "long" },
    { "name": "sam_number", "type": "string" },
    { "name": "_fivetran_synced", "type": "timestamp" },
    { "name": "_fivetran_deleted", "type": "boolean" },
    { "name": "date_deleted", "type": "timestamp" }
  ],
  "dependencies": [
    {
      "dbname": "edw.netsuite_sterling",
      "tablename": "transactions",
      "type": "table",
      "external": true
    },
    {
      "dbname": "edw.netsuite_sterling",
      "tablename": "netsuite_customer",
      "type": "table",
      "external": true
    }
  ]
}
