{
  "view": "org_attributes",
  "owner": "DataScience",
  "description": "Aggregate metrics for orgs to calculate cohorts / segments",
  "database": "dataproducts",
  "schema": [
    { "name": "org_id", "type": "long", "description": "$org_id_default_description" },
    {
      "name": "distance_driven_miles_per_vehicle",
      "type": "double",
      "description": "Distance driven (miles per vehcile)"
    },
    { "name": "trip_length", "type": "double", "description": "Average trip length" },
    { "name": "unique_active_vehicles", "type": "long", "description": "Unique active vehicles in org" },
    { "name": "percent_passenger", "type": "double", "description": "% of trips having a passenger" },
    { "name": "percent_trips_city", "type": "double", "description": "% of trips in the city" }
  ],
  "dependencies": [
    {
      "dbname": "dataproducts",
      "tablename": "fleet_driving_patterns",
      "type": "table"
    },
    {
      "dbname": "dataproducts",
      "tablename": "org_active_vgs",
      "type": "table"
    },
    {
      "dbname": "dataproducts",
      "tablename": "org_trip_city_count",
      "type": "table"
    }
  ]
}
