{
  "view": "fleet_driving_patterns",
  "owner": "DataScience",
  "description": " Aggegate driving pattern data for past 2 months. Total trip distance and number of trips per org as proxies for driving pattern",
  "database": "dataproducts",
  "schema": [
    { "name": "org_id", "type": "long", "description": "$org_id_default_description" },
    { "name": "total_trip_distance_miles", "type": "double", "description": "Total trip distance (miles)" },
    { "name": "num_trips", "type": "long", "nullable": false, "description": "Number of trips made" }
  ],
  "dependencies": [
    {
      "external": true,
      "dbname": "trips2db_shards",
      "tablename": "trips",
      "type": "table"
    }
  ]
}
