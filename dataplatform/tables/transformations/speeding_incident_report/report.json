{
  "name": "speeding_incident_report.report",
  "owner": "SafetyPlatform",
  "description": "Compute the speeding incidents per way per org.",
  "dependencies": [
    {
      "external": true,
      "dbname": "kinesisstats",
      "tablename": "location",
      "type": "table"
    }
  ],
  "expectation": {
    "batch_mode": "REMOVE_UNEXPECTED"
  },
  "sqlite_report_config": {
    "report_name": "speeding_incident_report",
    "staleness_monitors": [
      {
        "hours": 12,
        "urgency": "LOW"
      }
    ],
    "team_runbook_url": "https://samsara.atlassian-us-gov-mod.net/wiki/spaces/RD/pages/5253943/Runbook+speeding+incident+report+is+stale+for+4.5+hours"
  },
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "speeding_incident_report",
    "tablename": "report",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["date", "org_id", "way_id", "interval_start"],
    "schema": [
      {
        "name": "org_id",
        "type": "long"
      },
      {
        "name": "interval_start",
        "type": "timestamp"
      },
      {
        "name": "way_id",
        "type": "long"
      },
      {
        "name": "date",
        "type": "string"
      },
      {
        "name": "summed_observed_speed_mph",
        "type": "double"
      },
      {
        "name": "total_speeding_incidents",
        "type": "long",
        "nullable": false
      },
      {
        "name": "light_speeding_count_mph",
        "type": "long"
      },
      {
        "name": "light_speeding_count_kmph",
        "type": "long"
      },
      {
        "name": "moderate_speeding_count_mph",
        "type": "long"
      },
      {
        "name": "moderate_speeding_count_kmph",
        "type": "long"
      },
      {
        "name": "heavy_speeding_count_mph",
        "type": "long"
      },
      {
        "name": "heavy_speeding_count_kmph",
        "type": "long"
      },
      {
        "name": "severe_speeding_count_mph",
        "type": "long"
      },
      {
        "name": "severe_speeding_count_kmph",
        "type": "long"
      },
      {
        "name": "total_org_light_speeding_mph",
        "type": "long"
      },
      {
        "name": "total_org_moderate_speeding_mph",
        "type": "long"
      },
      {
        "name": "total_org_heavy_speeding_mph",
        "type": "long"
      },
      {
        "name": "total_org_severe_speeding_mph",
        "type": "long"
      },
      {
        "name": "total_org_light_speeding_kmph",
        "type": "long"
      },
      {
        "name": "total_org_moderate_speeding_kmph",
        "type": "long"
      },
      {
        "name": "total_org_heavy_speeding_kmph",
        "type": "long"
      },
      {
        "name": "total_org_severe_speeding_kmph",
        "type": "long"
      }
    ],
    "write_mode": "overwrite"
  }
}
