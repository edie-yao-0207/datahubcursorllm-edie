{
  "name": "engine_state_report.idling_by_hour",
  "owner": "Sustainability",
  "description": "Idling duration and fuel consumption aggregated by hour",
  "dependencies": [
    {
      "dbname": "engine_state_report",
      "tablename": "engine_state_pto_air_temp_fuel_intervals",
      "type": "table"
    }
  ],
  "parameters": ["start_date", "end_date"],
  "backfill_start_date": "2024-10-01",
  "output": {
    "dbname": "engine_state_report",
    "tablename": "idling_by_hour",
    "type": "table",
    "partition": ["date"],
    "write_mode": "overwrite",
    "primary_key": ["date", "org_id", "device_id", "start_ms"],
    "schema": [
      {
        "name": "date",
        "type": "date"
      },
      {
        "name": "org_id",
        "type": "long"
      },
      {
        "name": "device_id",
        "type": "long"
      },
      {
        "name": "start_ms",
        "type": "long",
        "nullable": false
      },
      {
        "name": "duration_ms",
        "type": "long"
      },
      {
        "name": "fuel_consumption_ml",
        "type": "double"
      },
      {
        "name": "gaseous_fuel_consumed_grams",
        "type": "double"
      }
    ]
  },
  "sqlite_report_config": {
    "report_name": "idling_report",
    "staleness_monitors": [
      {
        "hours": 13,
        "urgency": "LOW"
      },
      {
        "hours": 19,
        "urgency": "HIGH"
      }
    ],
    "team_runbook_url": "https://samsaradev.atlassian.net/wiki/x/XaMZmQ"
  }
}
