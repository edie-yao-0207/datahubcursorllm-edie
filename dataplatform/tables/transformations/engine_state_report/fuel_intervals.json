{
  "name": "engine_state_report.fuel_intervals",
  "owner": "Sustainability",
  "description": "Create fuel intervals by subtracting previous value from the current sum type fuel object stat.",
  "dependencies": [
    {
      "external": true,
      "dbname": "helpers",
      "tablename": "ignored_org_ids",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "kinesisstats",
      "tablename": "osdderivedfuelconsumed",
      "type": "table"
    }
  ],
  "parameters": ["start_date", "end_date"],
  "backfill_start_date": "2022-06-16",
  "output": {
    "dbname": "engine_state_report",
    "tablename": "fuel_intervals",
    "type": "table",
    "partition": ["date"],
