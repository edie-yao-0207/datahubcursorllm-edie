    "primary_key": ["date", "org_id", "object_id", "start_ms"],
    "schema": [
      {
        "name": "date",
        "type": "date"
      },
      {
        "name": "org_id",
        "type": "long"
      },
      {
        "name": "object_id",
        "type": "long"
      },
      {
        "name": "end_ms",
        "type": "long"
      },
      {
        "name": "start_ms",
        "type": "long"
      },
      {
        "name": "fuel_consumed_ml",
        "type": "long"
      },
      {
        "name": "original_interval_duration_ms",
        "type": "long",
        "metadata": {
          "comment": "The duration in milliseconds of the original interval from which this interval is derived."
        }
      }
    ]
  }
}
