{
  "name": "engine_state_report.engine_state_driver_intervals",
  "owner": "Sustainability",
  "description": "Combines driver id with average air temperature in celsius + engine state + PTO + fuel intervals.",
  "dependencies": [
    {
      "dbname": "engine_state_report",
      "tablename": "engine_state_pto_air_temp_fuel_intervals",
      "type": "table"
    },
    {
      "dbname": "fuel_energy_efficiency_report",
      "tablename": "driver_assignments",
      "type": "table"
    }
  ],
  "parameters": ["start_date", "end_date"],
  "backfill_start_date": "2024-10-01",
  "output": {
    "dbname": "engine_state_report",
    "tablename": "engine_state_driver_intervals",
    "type": "table",
    "partition": ["date"],
    "write_mode": "overwrite",
    "primary_key": ["date", "org_id", "object_id", "driver_id", "start_ms"],
    "schema": [
      {
        "name": "date",
        "type": "date"
      },
      {
        "name": "org_id",
        "type": "long"
      },
      {
        "name": "object_id",
        "type": "long"
      },
      {
        "name": "driver_id",
        "nullable": false,
        "type": "long",
        "metadata": {
          "comment": "$driver_id_default_description"
        }
      },
      {
        "name": "start_ms",
        "type": "long"
      },
      {
        "name": "end_ms",
        "type": "long"
      },
      {
        "name": "fuel_consumed_ml",
        "type": "double"
      },
      {
        "name": "gaseous_fuel_consumed_grams",
        "type": "double"
      },
      {
        "name": "productive_pto_state",
        "type": "long"
      },
      {
        "name": "engine_state",
        "type": "string"
      },
      {
        "name": "avg_air_temp_c",
        "type": "long"
      },
      {
        "name": "original_fuel_interval_duration_ms",
        "type": "long",
        "metadata": {
          "comment": "The duration in milliseconds of the original fuel interval from which this interval is derived."
        }
      },
      {
        "name": "original_engine_state_interval_duration_ms",
        "type": "long",
        "metadata": {
          "comment": "The duration in milliseconds of the original engine state interval from which this interval is derived."
        }
      },
      {
        "name": "original_pto_interval_duration_ms",
        "type": "long",
        "metadata": {
          "comment": "The duration in milliseconds of the original pto interval from which this interval is derived."
        }
      },
      {
        "name": "original_gaseous_consumption_interval_duration_ms",
        "type": "long",
        "metadata": {
          "comment": "The duration in milliseconds of the original gasoeus consumption interval from which this interval is derived."
        }
      }
    ]
  }
}
