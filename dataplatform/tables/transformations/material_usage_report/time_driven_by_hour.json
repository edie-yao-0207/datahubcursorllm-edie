{
  "name": "material_usage_report.time_driven_by_hour",
  "owner": "DiagnosticsTools",
  "description": "Calculates time driven for every spreader device by hour",
  "dependencies": [
    {
      "external": true,
      "dbname": "trips2db_shards",
      "tablename": "trips",
      "type": "table"
    },
    {
      "dbname": "material_usage_report",
      "tablename": "spreader_devices",
      "type": "table"
    }
  ],
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "material_usage_report",
    "tablename": "time_driven_by_hour",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["date", "org_id", "device_id", "hour_start"],
    "schema": [
      {
        "name": "date",
        "type": "date",
        "nullable": false
      },
      {
        "name": "org_id",
        "type": "long"
      },
      {
        "name": "device_id",
        "type": "long"
      },
      {
        "name": "hour_start",
        "type": "timestamp",
        "nullable": false
      },
      {
        "name": "hour_end",
        "type": "timestamp",
        "nullable": false
      },
      {
        "name": "duration_driven_ms",
        "type": "long"
      }
    ],
    "write_mode": "overwrite"
  },
  "expectation": {
    "batch_mode": "KEEP_ALL"
  },
  "backfill_start_date": "2021-12-01"
}
