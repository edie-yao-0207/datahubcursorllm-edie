        ],
        "material_usage_report.spreader_event_intervals_v2": [
          {
            "org_id": 1,
            "device_id": 1,
            "spreader_start_ms": 1577923190000,
            "spreader_end_ms": 1577923192000,
            "mode": 0,
            "date": "2020-01-01"
