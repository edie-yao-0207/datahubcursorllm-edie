{
  "name": "camera_connector_health.cc_reversing_and_recording_durations",
  "owner": "MediaExperience",
  "description": "Intervals when the camera connector is recording while reversing",
  "dependencies": [
    { "external": true, "dbname": "kinesisstats", "tablename": "osdvehiclecurrentgear", "type": "table" },
    { "external": true, "dbname": "kinesisstats", "tablename": "osdobdcableid", "type": "table" },
    { "external": true, "dbname": "kinesisstats", "tablename": "osdcanbustype", "type": "table" },
    { "external": true, "dbname": "attributedb_shards", "tablename": "attribute_values", "type": "table" },
    {
      "external": true,
      "dbname": "attributedb_shards",
      "tablename": "attribute_cloud_entities",
      "type": "table"
    },
    { "external": false, "dbname": "camera_connector_health", "tablename": "cc_recording_intervals", "type": "table" }
  ],
  "expectation": {
    "batch_mode": "KEEP_ALL"
  },
  "parameters": ["start_date", "end_date", "pipeline_execution_time"],
  "output": {
    "dbname": "camera_connector_health",
    "tablename": "cc_reversing_and_recording_durations",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["org_id", "vg_id", "reversing_start_ms"],
    "schema": [
      {
        "name": "org_id",
        "type": "long"
      },
      {
        "name": "date",
        "type": "string"
      },
      {
        "name": "vg_id",
        "type": "long",
        "metadata": { "comment": "Device ID of the VG emitting the osdvehiclecurrentgear object stat" }
      },
      {
        "name": "reversing_start_ms",
        "type": "long",
        "metadata": { "comment": "Unix timestamp of the start_ms of the reversing interval" }
      },
      {
        "name": "reversing_duration_ms",
        "type": "long",
        "metadata": { "comment": "Duration in milliseconds of the reversing interval" }
      },
      {
        "name": "recording_per_reversing_ms",
        "type": "long",
        "metadata": {
          "comment": "Duration in milliseconds that the camera connector has been recording during the reversing interval"
        }
      }
    ],
    "write_mode": "overwrite"
  }
}
