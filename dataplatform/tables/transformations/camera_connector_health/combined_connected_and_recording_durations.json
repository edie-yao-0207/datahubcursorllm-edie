{
  "name": "camera_connector_health.combined_connected_and_recording_durations",
  "owner": "MediaExperience",
  "description": "Intervals when the camera connector is recording with signal lock",
  "dependencies": [
    {
      "external": true,
      "dbname": "productsdb",
      "tablename": "devices",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "productsdb",
      "tablename": "gateways",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "deviceassociationsdb_shards",
      "tablename": "device_collection_associations",
      "type": "table"
    },
    {
      "external": false,
      "dbname": "camera_connector_health",
      "tablename": "cc_connected_and_recording_durations",
      "type": "table"
    },
    {
      "external": false,
      "dbname": "camera_connector_health",
      "tablename": "auxcam_connected_and_recording_durations",
      "type": "table"
    }
  ],
  "expectation": {
    "batch_mode": "FAIL_ALL"
  },
  "parameters": ["start_date", "end_date", "pipeline_execution_time"],
  "output": {
    "dbname": "camera_connector_health",
    "tablename": "combined_connected_and_recording_durations",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["org_id", "vg_id", "trip_start_ms"],
    "schema": [
      { "name": "org_id", "type": "long" },
      { "name": "date", "type": "string" },
      {
        "name": "vg_id",
        "type": "long",
        "metadata": { "comment": "Device ID of the VG connected to the Baxter or Auxcam device" }
      },
      {
        "name": "cm_id",
        "type": "long",
        "metadata": { "comment": "Device ID of the CM device connected to the VG, if available" }
      },
      {
        "name": "trip_start_ms",
        "type": "long",
        "metadata": { "comment": "Unix timestamp of the start_ms of the trip" }
      },
      {
        "name": "trip_duration_ms",
        "type": "long",
        "metadata": { "comment": "Duration in milliseconds of the trip" }
      },
      {
        "name": "connected_ms",
        "type": "long",
        "metadata": {
          "comment": "Duration in milliseconds that the camera connector has been connected during the trip"
        }
      },
      {
        "name": "recording_ms",
        "type": "long",
        "metadata": {
          "comment": "Duration in milliseconds that the camera connector has been recording during the trip"
        }
      }
    ],
    "write_mode": "overwrite"
  }
}
