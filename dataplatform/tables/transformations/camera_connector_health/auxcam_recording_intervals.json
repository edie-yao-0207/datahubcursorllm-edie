{
  "name": "camera_connector_health.auxcam_recording_intervals",
  "owner": "MediaExperience",
  "description": "Intervals when the camera connector is recording with signal lock",
  "dependencies": [
    {
      "external": true,
      "dbname": "deviceassociationsdb_shards",
      "tablename": "device_collection_associations",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "kinesisstats",
      "tablename": "osddashcamstate",
      "type": "table"
    }
  ],
  "expectation": {
    "batch_mode": "FAIL_ALL"
  },
  "parameters": ["start_date", "end_date", "pipeline_execution_time"],
  "output": {
    "dbname": "camera_connector_health",
    "tablename": "auxcam_recording_intervals",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["org_id", "auxcam_id", "start_ms"],
    "schema": [
      { "name": "org_id", "type": "long" },
      { "name": "date", "type": "string" },
      {
        "name": "auxcam_id",
        "type": "long",
        "metadata": { "comment": "Device ID of the Auxcam" }
      },
      {
        "name": "start_ms",
        "type": "long",
        "metadata": { "comment": "Unix timestamp of the start_ms of the interval" }
      },
      {
        "name": "end_ms",
        "type": "long",
        "metadata": { "comment": "Unix timestamp of the end_ms of the interval" }
      }
    ],
    "write_mode": "overwrite"
  }
}
