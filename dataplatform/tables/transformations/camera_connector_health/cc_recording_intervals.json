{
  "name": "camera_connector_health.cc_recording_intervals",
  "owner": "MediaExperience",
  "description": "Intervals when the camera connector is recording with signal lock",
  "dependencies": [
    {
      "external": true,
      "dbname": "productsdb",
      "tablename": "devices",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "productsdb",
      "tablename": "gateways",
      "type": "table"
    },
    { "external": true, "dbname": "kinesisstats", "tablename": "osdrearcameraframestate", "type": "table" }
  ],
  "expectation": {
    "batch_mode": "REMOVE_UNEXPECTED"
  },
  "parameters": ["start_date", "end_date", "pipeline_execution_time"],
  "output": {
    "dbname": "camera_connector_health",
    "tablename": "cc_recording_intervals",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["org_id", "cm_id", "vg_id", "start_ms"],
    "schema": [
      { "name": "org_id", "type": "long" },
      { "name": "date", "type": "string" },
      {
        "name": "cm_id",
        "type": "long",
        "metadata": { "comment": "Device ID of the CM emitting the osdrearcameraframestate object stat" }
      },
      {
        "name": "vg_id",
        "type": "long",
        "metadata": { "comment": "Device ID of the VG device connected to the CM" }
      },
      {
        "name": "start_ms",
        "type": "long",
        "metadata": { "comment": "Unix timestamp of the start_ms of the interval" }
      },
      {
        "name": "end_ms",
        "type": "long",
        "metadata": { "comment": "Unix timestamp of the end_ms of the interval" }
      }
    ],
    "write_mode": "overwrite"
  }
}
