{
  "name": "camera_connector_health.auxcam_connected_and_recording_durations",
  "owner": "MediaExperience",
  "description": "Intervals when the camera connector is recording with signal lock",
  "dependencies": [
    {
      "external": true,
      "dbname": "deviceassociationsdb_shards",
      "tablename": "device_collection_associations",
      "type": "table"
    },
    { "external": true, "dbname": "kinesisstats", "tablename": "osdmulticamconnected", "type": "table" },
    { "external": true, "dbname": "kinesisstats", "tablename": "osddashcamconnected", "type": "table" },
    { "external": true, "dbname": "trips2db_shards", "tablename": "trips", "type": "table" },
    {
      "external": false,
      "dbname": "camera_connector_health",
      "tablename": "auxcam_recording_intervals",
      "type": "table"
    }
  ],
  "expectation": {
    "batch_mode": "FAIL_ALL"
  },
  "spark_execution_environment": {
    "spark_version": "13.3.x-scala2.12"
  },
  "parameters": ["start_date", "end_date", "pipeline_execution_time"],
  "output": {
    "dbname": "camera_connector_health",
    "tablename": "auxcam_connected_and_recording_durations",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["org_id", "auxcam_id", "vg_id", "trip_start_ms"],
    "schema": [
      { "name": "org_id", "type": "long" },
      { "name": "date", "type": "string" },
      {
        "name": "vg_id",
        "type": "long",
        "metadata": { "comment": "Device ID of the VG emitting the osdrearcameraconnected object stat" }
      },
      {
        "name": "auxcam_id",
        "type": "long",
        "metadata": { "comment": "Device ID of the Auxcam device connected to the VG" }
      },
      {
        "name": "trip_start_ms",
        "type": "long",
        "metadata": { "comment": "Unix timestamp of the start_ms of the trip" }
      },
      {
        "name": "trip_duration_ms",
        "type": "long",
        "metadata": { "comment": "Duration in milliseconds of the trip" }
      },
      {
        "name": "connected_ms",
        "type": "long",
        "metadata": {
          "comment": "Duration in milliseconds that the camera connector has been connected during the trip"
        },
        "nullable": false
      },
      {
        "name": "recording_ms",
        "type": "long",
        "metadata": {
          "comment": "Duration in milliseconds that the camera connector has been recording during the trip"
        },
        "nullable": false
      }
    ],
    "write_mode": "overwrite"
  }
}
