{
  "name": "ecodriving_report.uphill_and_downhill_duration_by_hour",
  "owner": "Sustainability",
  "description": "Determines the vehicle travel durations on uphill and downhill inclines in milliseconds over hourly buckets.",
  "dependencies": [
    {
      "dbname": "kinesisstats_window",
      "tablename": "location",
      "type": "table"
    },
    {
      "dbname": "engine_state",
      "tablename": "intervals",
      "type": "table"
    },
    {
      "dbname": "canonical_distance",
      "tablename": "total_distance",
      "type": "table"
    },
    {
      "dbname": "fuel_energy_efficiency_report",
      "tablename": "driver_assignments",
      "type": "table"
    }
  ],
  "tags": ["ecodriving"],
  "parameters": ["start_date", "end_date"],
  "backfill_start_date": "2024-08-01",
  "output": {
    "dbname": "ecodriving_report",
    "tablename": "uphill_and_downhill_duration_by_hour",
    "type": "table",
    "partition": ["date"],
    "write_mode": "overwrite",
    "primary_key": ["date", "org_id", "device_id", "driver_id", "interval_start"],
    "schema": [
      {
        "name": "date",
        "type": "string",
        "metadata": {
          "comment": "The date of the hourly interval."
        }
      },
      {
        "name": "org_id",
        "type": "long",
        "metadata": {
          "comment": "$org_id_default_description"
        }
      },
      {
        "name": "device_id",
        "type": "long",
        "metadata": {
          "comment": "$device_id_default_description"
        }
      },
      {
        "name": "driver_id",
        "nullable": false,
        "type": "long",
        "metadata": {
          "comment": "$driver_id_default_description"
        }
      },
      {
        "name": "interval_start",
        "type": "timestamp",
        "metadata": {
          "comment": "The timestamp at the beginning of this hourly bucket."
        }
      },
      {
        "name": "interval_end",
        "type": "timestamp",
        "metadata": {
          "comment": "The timestamp at the end of this hourly bucket."
        }
      },
      {
        "name": "uphill_duration_ms",
        "type": "long",
        "metadata": {
          "comment": "Sum of the millisecond duration spent on driving in uphill incline during this hourly interval for a device and a driver."
        }
      },
      {
        "name": "downhill_duration_ms",
        "type": "long",
        "metadata": {
          "comment": "Sum of the millisecond duration spent on driving in downhill incline during this hourly interval for a device and a driver."
        }
      }
    ]
  }
}
