{
  "name": "ecodriving_report.braking_duration_ms",
  "owner": "Sustainability",
  "description": "Aggregate duration of braking (of any type) within a period.",
  "dependencies": [
    {
      "external": true,
      "dbname": "kinesisstats",
      "tablename": "osDRegenBrakingMs",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "kinesisstats",
      "tablename": "osDAggregateDurationRetarderBrakingMs",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "kinesisstats",
      "tablename": "osDAggregateDurationFrictionBrakingMs",
      "type": "table"
    },
    {
      "external": false,
      "dbname": "fuel_energy_efficiency_report",
      "tablename": "driver_assignments",
      "type": "table"
    }
  ],
  "tags": ["ecodriving"],
  "parameters": ["start_date", "end_date"],
  "backfill_start_date": "2024-08-01",
  "output": {
    "dbname": "ecodriving_report",
    "tablename": "braking_duration_ms",
    "type": "table",
    "partition": ["date"],
    "write_mode": "overwrite",
    "primary_key": ["date", "org_id", "device_id", "driver_id", "interval_start", "interval_end"],
    "schema": [
      {
        "name": "date",
        "type": "string",
        "nullable": true,
        "metadata": {
          "comment": "The date of the hourly interval."
        }
      },
      {
        "name": "org_id",
        "type": "long",
        "metadata": {
          "comment": "$org_id_default_description"
        }
      },
      {
        "name": "device_id",
        "type": "long",
        "metadata": {
          "comment": "$device_id_default_description"
        }
      },
      {
        "name": "interval_start",
        "type": "timestamp",
        "nullable": true,
        "metadata": {
          "comment": "The timestamp at the beginning of this hourly bucket."
        }
      },
      {
        "name": "interval_end",
        "type": "timestamp",
        "nullable": true,
        "metadata": {
          "comment": "The timestamp at the end of this hourly bucket."
        }
      },
      {
        "name": "duration_ms",
        "type": "long",
        "nullable": true,
        "metadata": {
          "comment": "The duration of braking (of any type) during this interval."
        }
      },
      {
        "name": "driver_id",
        "type": "long",
        "nullable": false,
        "metadata": {
          "comment": "The ID of the driver assigned to this trip (0 if no driver assigned)."
        }
      }
    ]
  }
}
