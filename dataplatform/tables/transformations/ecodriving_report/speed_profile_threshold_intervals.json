{
  "name": "ecodriving_report.speed_profile_threshold_intervals",
  "owner": "Sustainability",
  "description": "Obtains the speed threshold for profiles over hourly buckets.",
  "dependencies": [
    {
      "external": true,
      "dbname": "fueldb_shards",
      "tablename": "profile_speed_thresholds",
      "type": "table"
    }
  ],
  "tags": ["ecodriving"],
  "parameters": ["start_date", "end_date"],
  "backfill_start_date": "2024-08-01",
  "output": {
    "dbname": "ecodriving_report",
    "tablename": "speed_profile_threshold_intervals",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["date", "org_id", "profile_id", "interval_start"],
    "schema": [
      {
        "name": "date",
        "type": "date",
        "nullable": false,
        "metadata": {
          "comment": "The date of the hourly interval."
        }
      },
      {
        "name": "org_id",
        "type": "long",
        "metadata": {
          "comment": "The Samsara ID of the organization that the data, device and driver belong to."
        }
      },
      {
        "name": "profile_id",
        "type": "string",
        "metadata": {
          "comment": "The unique ID of an ecodriving configuration profile."
        }
      },
      {
        "name": "interval_start",
        "type": "timestamp",
        "nullable": false,
        "metadata": {
          "comment": "The timestamp at the beginning of this hourly bucket."
        }
      },
      {
        "name": "interval_end",
        "type": "timestamp",
        "nullable": false,
        "metadata": {
          "comment": "The timestamp at the end of this hourly bucket."
        }
      },
      {
        "name": "custom_speed_profile_threshold_meters_per_second",
        "type": "double",
        "metadata": {
          "comment": "The speeding threshold in meters per second attributed to this profile id during this hourly interval."
        }
      }
    ],
    "write_mode": "merge"
  }
}
