{
  "name": "objectstat_diffs.cruise_control",
  "owner": "Sustainability",
  "description": "Calculate cruise control diffs from kinesisstats.osdderivedcruisecontrolms object stat",
  "dependencies": [
    {
      "external": true,
      "dbname": "kinesisstats",
      "tablename": "osdderivedcruisecontrolms",
      "type": "table"
    }
  ],
  "tags": ["ecodriving"],
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "objectstat_diffs",
    "tablename": "cruise_control",
    "type": "table",
    "partition": ["date"],
    "write_mode": "overwrite",
    "primary_key": ["date", "org_id", "object_id", "time"],
    "schema": [
      {
        "name": "date",
        "type": "date",
        "metadata": {
          "comment": "The date when cruise control duration was reported."
        }
      },
      {
        "name": "org_id",
        "type": "long",
        "metadata": {
          "comment": "$org_id_default_description"
        }
      },
      {
        "name": "object_id",
        "type": "long",
        "metadata": {
          "comment": "The ID of the Samsara device from which cruise control duration was recorded."
        }
      },
      {
        "name": "time",
        "type": "long",
        "metadata": {
          "comment": "The end time in milliseconds at which cruise control duration was recorded."
        }
      },
      {
        "name": "cruise_control_ms",
        "type": "long",
        "metadata": {
          "comment": "Vehicle duration of cruise control being enabled (ms)."
        }
      }
    ]
  }
}
