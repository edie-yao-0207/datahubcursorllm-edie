{
  "name": "objectstat_diffs.rpm_green_band",
  "owner": "Sustainability",
  "description": "Calculate green band diffs from kinesisstats.osdderivedrpmgreenbandms object stat",
  "dependencies": [
    {
      "external": true,
      "dbname": "kinesisstats",
      "tablename": "osdderivedrpmgreenbandms",
      "type": "table"
    }
  ],
  "tags": ["ecodriving"],
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "objectstat_diffs",
    "tablename": "rpm_green_band",
    "type": "table",
    "partition": ["date"],
    "write_mode": "overwrite",
    "primary_key": ["date", "org_id", "object_id", "time"],
    "schema": [
      {
        "name": "date",
        "type": "date",
        "metadata": {
          "comment": "The date when the green band period was recorded."
        }
      },
      {
        "name": "org_id",
        "type": "long",
        "metadata": {
          "comment": "$org_id_default_description"
        }
      },
      {
        "name": "object_id",
        "type": "long",
        "metadata": {
          "comment": "The ID of the Samsara device from which the green band period was recorded."
        }
      },
      {
        "name": "time",
        "type": "long",
        "metadata": {
          "comment": "The time in milliseconds at which the green band period was recorded."
        }
      },
      {
        "name": "rpm_green_band_ms",
        "type": "long",
        "metadata": {
          "comment": "The green band duration in milliseconds."
        }
      }
    ]
  }
}
