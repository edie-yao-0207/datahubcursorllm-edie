{
  "name": "objectstat_diffs.coasting",
  "owner": "Sustainability",
  "description": "Calculate coasting diffs from kinesisstats.osdderivedcoastingtimems object stat",
  "dependencies": [
    {
      "external": true,
      "dbname": "kinesisstats",
      "tablename": "osdderivedcoastingtimems",
      "type": "table"
    }
  ],
  "tags": ["ecodriving"],
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "objectstat_diffs",
    "tablename": "coasting",
    "type": "table",
    "partition": ["date"],
    "write_mode": "overwrite",
    "primary_key": ["date", "org_id", "object_id", "time"],
    "schema": [
      {
        "name": "date",
        "type": "date",
        "metadata": {
          "comment": "The date when coasting time was reported."
        }
      },
      {
        "name": "org_id",
        "type": "long",
        "metadata": {
          "comment": "$org_id_default_description"
        }
      },
      {
        "name": "object_id",
        "type": "long",
        "metadata": {
          "comment": "The ID of the Samsara device from which coasting time was recorded."
        }
      },
      {
        "name": "time",
        "type": "long",
        "metadata": {
          "comment": "The end time in milliseconds at which coasting time was recorded."
        }
      },
      {
        "name": "coasting_time_ms",
        "type": "long",
        "metadata": {
          "comment": "Vehicle coasting time detected (ms)."
        }
      }
    ]
  }
}
