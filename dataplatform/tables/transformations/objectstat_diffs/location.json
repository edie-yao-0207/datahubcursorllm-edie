{
  "name": "objectstat_diffs.location",
  "owner": "TelematicsData",
  "description": "Calculates distance traveled between two consecutive points from kinesisstats_window.location, as calculated by difference in latitude and longitude.",
  "dependencies": [
    {
      "dbname": "kinesisstats_window",
      "tablename": "location",
      "type": "table"
    }
  ],
  "spark_execution_environment": {
    "spark_version": "13.3.x-scala2.12",
    "s3_init_scripts": ["sedona-shaded-1.4.1-init.sh"]
  },
  "parameters": ["start_date", "end_date"],
  "backfill_start_date": "2021-01-31",
  "output": {
    "dbname": "objectstat_diffs",
    "tablename": "location",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["date", "org_id", "device_id", "time"],
    "schema": [
      {
        "name": "org_id",
        "type": "long",
        "metadata": {
          "comment": "$org_id_default_description"
        }
      },
      {
        "name": "date",
        "type": "string",
        "metadata": {
          "comment": "The date on which this change in location was recorded."
        }
      },
      {
        "name": "device_id",
        "type": "long",
        "metadata": {
          "comment": "The device id of this data point."
        }
      },
      {
        "name": "time",
        "type": "long",
        "metadata": {
          "comment": "The time in milliseconds at which this distance change was recorded."
        }
      },
      {
        "name": "distance",
        "type": "double",
        "metadata": {
          "comment": "The distance traveled in meters according to the difference between latitude/longitude coordinates of this point in time and the previous point in time."
        }
      }
    ],
    "write_mode": "overwrite"
  },
  "expectation": {
    "batch_mode": "KEEP_ALL"
  }
}
