{
  "name": "objectstat_diffs.brake_events",
  "owner": "Sustainability",
  "description": "Calculate break event diffs from kinesisstats.osdderivedbrakeevents object stat",
  "dependencies": [
    {
      "external": true,
      "dbname": "kinesisstats",
      "tablename": "osdderivedbrakeevents",
      "type": "table"
    }
  ],
  "tags": ["ecodriving"],
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "objectstat_diffs",
    "tablename": "brake_events",
    "type": "table",
    "partition": ["date"],
    "write_mode": "overwrite",
    "primary_key": ["date", "org_id", "object_id", "time"],
    "schema": [
      {
        "name": "date",
        "type": "date",
        "metadata": {
          "comment": "The date when the break event was reported."
        }
      },
      {
        "name": "org_id",
        "type": "long",
        "metadata": {
          "comment": "$org_id_default_description"
        }
      },
      {
        "name": "object_id",
        "type": "long",
        "metadata": {
          "comment": "The ID of the Samsara device from which the break event was recorded."
        }
      },
      {
        "name": "time",
        "type": "long",
        "metadata": {
          "comment": "The time in milliseconds at which the break event was recorded."
        }
      },
      {
        "name": "brake_events",
        "type": "long",
        "metadata": {
          "comment": "The number of brake events happened during the past 300 seconds."
        }
      }
    ]
  }
}
