{
  "name": "objectstat_diffs.limited_coasting",
  "owner": "Sustainability",
  "description": "Calculate limited coasting diffs from kinesisstats.osdderivedcoastingtimetorquebasedwhilenotoncruisecontrolms object stat",
  "dependencies": [
    {
      "external": true,
      "dbname": "kinesisstats",
      "tablename": "osdderivedcoastingtimetorquebasedwhilenotoncruisecontrolms",
      "type": "table"
    }
  ],
  "tags": ["ecodriving"],
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "objectstat_diffs",
    "tablename": "limited_coasting",
    "type": "table",
    "partition": ["date"],
    "write_mode": "overwrite",
    "primary_key": ["date", "org_id", "object_id", "time"],
    "schema": [
      {
        "name": "date",
        "type": "date",
        "metadata": {
          "comment": "The date when limited coasting time was reported."
        }
      },
      {
        "name": "org_id",
        "type": "long",
        "metadata": {
          "comment": "$org_id_default_description"
        }
      },
      {
        "name": "object_id",
        "type": "long",
        "metadata": {
          "comment": "The ID of the Samsara device from which limited coasting time was recorded."
        }
      },
      {
        "name": "time",
        "type": "long",
        "metadata": {
          "comment": "The end time in milliseconds at which limited coasting time was recorded."
        }
      },
      {
        "name": "limited_coasting_time_ms",
        "type": "long",
        "metadata": {
          "comment": "Total vehicle coasting time (ms) detected during the past 300 seconds."
        }
      }
    ]
  }
}
