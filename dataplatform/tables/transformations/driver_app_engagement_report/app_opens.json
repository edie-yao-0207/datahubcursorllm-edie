{
  "name": "driver_app_engagement_report.app_opens",
  "owner": "Mobile",
  "description": "Query for mobile logs to determine a count of driver app opens for hourly intervals.",
  "dependencies": [{ "dbname": "datastreams", "tablename": "mobile_logs", "type": "table", "external": true }],
  "parameters": ["start_date", "end_date"],
  "backfill_start_date": "2020-10-01",
  "sqlite_report_config": {
    "report_name": "driver_app_engagement_report",
    "staleness_monitors": [
      {
        "disable": true
      }
    ]
  },
  "output": {
    "dbname": "driver_app_engagement_report",
    "tablename": "app_opens",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["date", "org_id", "driver_id", "interval_start"],
    "schema": [
      {
        "name": "date",
        "type": "date",
        "metadata": { "comment": "The date corresponding to the interval start time." }
      },
      {
        "name": "org_id",
        "type": "long",
        "metadata": { "comment": "$org_id_default_description" }
      },
      {
        "name": "driver_id",
        "type": "long",
        "metadata": { "comment": "The Samsara ID for the driver that the mobile logs originate from." }
      },
      {
        "name": "interval_start",
        "type": "timestamp",
        "metadata": { "comment": "The timestamp of the start of the hourly interval." }
      },
      {
        "name": "num_app_opens",
        "type": "long",
        "metadata": { "comment": "The count of driver app opens during the hourly interval." }
      }
    ],
    "write_mode": "merge"
  }
}
