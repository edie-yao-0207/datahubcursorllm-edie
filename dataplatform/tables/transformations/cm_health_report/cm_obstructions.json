{
  "name": "cm_health_report.cm_obstructions",
  "owner": "MediaExperience",
  "description": "The latest inward and outward obstructions on cameras",
  "dependencies": [{ "external": true, "dbname": "safetydb_shards", "tablename": "safety_events", "type": "table" }],
  "expectation": {
    "batch_mode": "REMOVE_UNEXPECTED"
  },
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "cm_health_report",
    "tablename": "cm_obstructions",
    "type": "table",
    "partition": [],
    "primary_key": ["org_id", "device_id"],
    "schema": [
      { "name": "org_id", "type": "long" },
      { "name": "device_id", "type": "long" },
      {
        "name": "latest_outward_detection_ms",
        "type": "long",
        "metadata": {
          "comment": "The event_ms of the last outward obstruction detected on a device"
        }
      },
      {
        "name": "latest_inward_detection_ms",
        "type": "long",
        "metadata": {
          "comment": "The event_ms of the last inward obstruction detected on a device"
        }
      },
      {
        "name": "latest_misaligned_detection_ms",
        "type": "long",
        "metadata": {
          "comment": "The event_ms of the last camera misaligned detected on a device"
        }
      }
    ],
    "write_mode": "overwrite"
  }
}
