{
  "name": "cm_health_report.cm_power_intervals",
  "owner": "MediaExperience",
  "description": "Time intervals for which a CM3X was powered on. CM2Xs do not report this stat.",
  "dependencies": [
    { "external": false, "dbname": "cm_health_report", "tablename": "cm_2x_3x_linked_vgs", "type": "table" },
    { "external": true, "dbname": "kinesisstats", "tablename": "osdpowerstate", "type": "table" }
  ],
  "expectation": {
    "batch_mode": "FAIL_ALL"
  },
  "parameters": ["start_date", "end_date", "pipeline_execution_time"],
  "output": {
    "dbname": "cm_health_report",
    "tablename": "cm_power_intervals",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["org_id", "device_id", "linked_cm_id", "start_ms"],
    "schema": [
      { "name": "org_id", "type": "long" },
      { "name": "device_id", "type": "long" },
      { "name": "linked_cm_id", "type": "long" },
      { "name": "start_ms", "type": "long" },
      { "name": "has_power", "type": "boolean", "nullable": false },
      { "name": "end_ms", "type": "long" },
      { "name": "date", "type": "string" }
    ],
    "write_mode": "overwrite"
  }
}
