{
  "name": "cm_health_report.cm_recording_intervals",
  "owner": "MediaExperience",
  "description": "Time intervals for which a CM was marked as connected to a VG and recording.",
  "dependencies": [
    { "external": false, "dbname": "cm_health_report", "tablename": "cm_vg_build_and_bootcount", "type": "table" },
    { "external": false, "dbname": "cm_health_report", "tablename": "cm_2x_3x_linked_vgs", "type": "table" },
    { "external": false, "dbname": "cm_health_report", "tablename": "cm_vg_intervals", "type": "table" },
    {
      "external": false,
      "dbname": "cm_health_report",
      "tablename": "cm_physically_disconnected_intervals",
      "type": "table"
    },
    { "external": true, "dbname": "kinesisstats", "tablename": "osddashcamstate", "type": "table" }
  ],
  "expectation": {
    "batch_mode": "REMOVE_UNEXPECTED"
  },
  "parameters": ["start_date", "end_date", "pipeline_execution_time"],
  "output": {
    "dbname": "cm_health_report",
    "tablename": "cm_recording_intervals",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["org_id", "device_id", "start_ms"],
    "schema": [
      { "name": "org_id", "type": "long" },
      {
        "name": "device_id",
        "type": "long",
        "metadata": {
          "comment": "VG device id"
        }
      },
      { "name": "cm_device_id", "type": "long" },
      {
        "name": "product_id",
        "type": "long",
        "metadata": {
          "comment": "VG product id"
        }
      },
      { "name": "cm_product_id", "type": "long" },
      {
        "name": "start_ms",
        "type": "long",
        "metadata": {
          "comment": "Unix timestamp of the start_ms of the interval. Intervals are broken up by on trip and off trip. See on_trip to determine if the interval is on or off trip."
        }
      },
      {
        "name": "end_ms",
        "type": "long",
        "metadata": {
          "comment": "Unix timestamp of the end_ms of the interval. Intervals are broken up by on trip and off trip. See on_trip to determine if the interval is on or off trip."
        }
      },
      {
        "name": "duration_ms",
        "type": "long",
        "metadata": {
          "comment": "Duration of the interval in milliseconds"
        }
      },
      { "name": "date", "type": "string" },
      {
        "name": "interval_connected_duration_ms",
        "type": "long",
        "metadata": {
          "comment": "The duration in milliseconds the CM was connected to the VG during the interval"
        }
      },
      {
        "name": "interval_recording_start_ms",
        "type": "long",
        "metadata": {
          "comment": "The unix timestamp the CM first began recording for the interval"
        }
      },
      {
        "name": "on_trip",
        "type": "boolean",
        "metadata": {
          "comment": "True if the device was on a trip for the interval, false if the device was not on a trip"
        }
      },
      {
        "name": "grace_recording_duration_ms",
        "type": "long",
        "metadata": {
          "comment": "The same as interval_recording_start_ms, but we potentially extend the recording duration to the start of the trip if we see recording start within 90s of the trip/interval start"
        }
      },
      {
        "name": "recording_duration_ms",
        "type": "long",
        "metadata": {
          "comment": "The duration in milliseconds the CM was recording to the VG during the interval"
        }
      },
      {
        "name": "vg_bootcount",
        "type": "integer",
        "nullable": false,
        "metadata": {
          "comment": "The number of times the VG rebooted during the interval"
        }
      },
      {
        "name": "vg_build",
        "type": "string",
        "metadata": {
          "comment": "The firmware build that the VG was on during the interval"
        }
      },
      {
        "name": "cm_bootcount",
        "type": "integer",
        "nullable": false,
        "metadata": {
          "comment": "The number of times the CM rebooted during the interval"
        }
      },
      {
        "name": "cm_build",
        "type": "string",
        "metadata": {
          "comment": "The firmware build that the CM was on during the interval"
        }
      }
    ],
    "write_mode": "overwrite"
  }
}
