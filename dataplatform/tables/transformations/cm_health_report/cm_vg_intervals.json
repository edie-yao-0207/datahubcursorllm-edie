{
  "name": "cm_health_report.cm_vg_intervals",
  "owner": "MediaExperience",
  "description": "Trip history for VGs and attached CMs with trips broken down by the hour.",
  "dependencies": [
    { "external": false, "dbname": "cm_health_report", "tablename": "cm_2x_3x_linked_vgs", "type": "table" },
    { "external": true, "dbname": "trips2db_shards", "tablename": "trips", "type": "table" }
  ],
  "expectation": {
    "batch_mode": "REMOVE_UNEXPECTED"
  },
  "parameters": ["start_date", "end_date", "pipeline_execution_time"],
  "output": {
    "dbname": "cm_health_report",
    "tablename": "cm_vg_intervals",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["org_id", "device_id", "start_ms"],
    "schema": [
      { "name": "org_id", "type": "long" },
      {
        "name": "device_id",
        "type": "long",
        "metadata": {
          "comment": "VG device id"
        }
      },
      {
        "name": "product_id",
        "type": "long",
        "metadata": {
          "comment": "VG product id"
        }
      },
      { "name": "cm_device_id", "type": "long" },
      { "name": "cm_product_id", "type": "long" },
      {
        "name": "start_ms",
        "type": "long",
        "metadata": {
          "comment": "Unix timestamp of the start_ms of the interval. See on_trip to determine if the interval is on or off trip."
        }
      },
      {
        "name": "end_ms",
        "type": "long",
        "metadata": {
          "comment": "Unix timestamp of the end_ms of the interval. See on_trip to determine if the interval is on or off trip."
        }
      },
      {
        "name": "duration_ms",
        "type": "long",
        "metadata": {
          "comment": "Duration of the interval in milliseconds"
        }
      },
      { "name": "date", "type": "string" },
      {
        "name": "on_trip",
        "type": "boolean",
        "nullable": false,
        "metadata": {
          "comment": "True if the device was on a trip for the interval, false if the device was not on a trip"
        }
      }
    ],
    "write_mode": "overwrite"
  }
}
