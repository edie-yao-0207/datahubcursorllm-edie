{
  "name": "cm_health_report.cm_physically_disconnected_intervals",
  "owner": "MediaExperience",
  "description": "Time intervals for which a CM was physically disconnected from a linked VG. For CM 3X's, this takes into account both attached usb devices and power state. For CM2X's, this only takes into account attached USB devices.",
  "dependencies": [
    { "external": false, "dbname": "cm_health_report", "tablename": "cm_power_intervals", "type": "table" },
    { "external": false, "dbname": "cm_health_report", "tablename": "cm_attached_intervals", "type": "table" }
  ],
  "expectation": {
    "batch_mode": "FAIL_ALL"
  },
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "cm_health_report",
    "tablename": "cm_physically_disconnected_intervals",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["org_id", "device_id", "start_ms"],
    "schema": [
      { "name": "org_id", "type": "long" },
      { "name": "linked_cm_id", "type": "long" },
      { "name": "device_id", "type": "long" },
      { "name": "start_ms", "type": "long" },
      { "name": "end_ms", "type": "long" },
      { "name": "date", "type": "string" }
    ],
    "write_mode": "overwrite"
  }
}
