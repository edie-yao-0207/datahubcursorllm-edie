{
  "name": "dataplatform_stable_report.report",
  "owner": "DataPlatform",
  "description": "Computes a hacky version of a wifi usage report.",
  "backfill_start_date": "2021-05-01",
  "daily_run_config": {
    "start_date_offset": 4
  },
  "dependencies": [
    {
      "dbname": "dataplatform_stable_report",
      "tablename": "metered_usage",
      "type": "table"
    },
    {
      "dbname": "dataplatform_stable_report",
      "tablename": "whitelist_usage",
      "type": "table"
    }
  ],
  "parameters": ["start_date", "end_date"],
  "sqlite_report_config": {
    "report_name": "dataplatform_stable_report",
    "team_runbook_url": "https://samsara.atlassian-us-gov-mod.net/wiki/spaces/RD/pages/4540225/Runbook+Report+has+been+stale+X+hours",
    "staleness_monitors": [
      {
        "hours": 15,
        "urgency": "LOW"
      },
      {
        "hours": 7,
        "urgency": "LOW"
      }
    ]
  },
  "output": {
    "dbname": "dataplatform_stable_report",
    "tablename": "report",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["date", "org_id", "device_id", "interval_start"],
    "schema": [
      {
        "name": "date",
        "type": "string"
      },
      {
        "name": "org_id",
        "type": "long"
      },
      {
        "name": "device_id",
        "type": "long"
      },
      {
        "name": "interval_start",
        "type": "timestamp"
      },
      {
        "name": "metered_usage_bytes",
        "type": "long",
        "nullable": false
      },
      {
        "name": "whitelist_usage_bytes",
        "type": "long",
        "nullable": false
      }
    ],
    "write_mode": "merge"
  },
  "pipeline_config": {
    "developer_error_severity": "HIGH"
  }
}
