{
  "name": "engine_state.intervals_by_hour",
  "owner": "Sustainability",
  "description": "Splits engine state intervals by hours at the clock hour boundary",
  "dependencies": [
    {
      "dbname": "engine_state",
      "tablename": "intervals",
      "type": "table"
    }
  ],
  "parameters": ["start_date", "end_date"],
  "backfill_start_date": "2024-08-01",
  "output": {
    "dbname": "engine_state",
    "tablename": "intervals_by_hour",
    "partition": ["date"],
    "type": "table",
    "primary_key": ["date", "org_id", "object_id", "start_ms"],
    "schema": [
      {
        "name": "date",
        "type": "date",
        "nullable": false
      },
      {
        "name": "org_id",
        "type": "long"
      },
      {
        "name": "object_id",
        "type": "long"
      },
      {
        "name": "state",
        "type": "string"
      },
      {
        "name": "start_ms",
        "type": "long"
      },
      {
        "name": "end_ms",
        "type": "long"
      }
    ],
    "write_mode": "overwrite"
  }
}
