{
  "name": "dataplatform_beta_report.metered_usage",
  "owner": "DataPlatform",
  "description": "Computes hourly rows for metered usage of wifi for vehicles.",
  "backfill_start_date": "2022-01-02",
  "nonproduction": true,
  "dependencies": [
    {
      "external": true,
      "dbname": "kinesisstats",
      "tablename": "osdwifiapbytes",
      "type": "table"
    }
  ],
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "dataplatform_beta_report",
    "tablename": "metered_usage",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["date", "org_id", "device_id", "interval_start"],
    "schema": [
      {
        "name": "date",
        "type": "string"
      },
      {
        "name": "org_id",
        "type": "long",
        "metadata": {
          "comment": "$org_id_default_description"
        }
      },
      {
        "name": "device_id",
        "type": "long"
      },
      {
        "name": "interval_start",
        "type": "timestamp"
      },
      {
        "name": "metered_usage_bytes",
        "type": "long"
      }
    ],
    "write_mode": "merge"
  }
}
