{
  "name": "data_analytics.baxter_devices",
  "owner": "DataAnalytics",
  "description": "This table contains ID data for Baxter devices that have been connected in the last two weeks from execution date",
  "nonproduction": true,
  "dependencies": [
    {
      "external": true,
      "dbname": "kinesisstats",
      "tablename": "osdrearcameraconnected",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "kinesisstats",
      "tablename": "osdrearcameraframestate",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "dataprep_safety",
      "tablename": "cm_linked_vgs",
      "schema_path": "dataprep/safety/dataprep_cm_linked_vgs.metadata.json",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "productsdb",
      "tablename": "devices",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "attributedb_shards",
      "tablename": "attribute_cloud_entities",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "attributedb_shards",
      "tablename": "attribute_values",
      "type": "table"
    }
  ],
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "data_analytics",
    "tablename": "baxter_devices",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["date", "org_id", "vg_device_id", "cm_device_id"],
    "schema": [
      {
        "name": "date",
        "type": "string",
        "nullable": true,
        "metadata": {
          "comment": "The date on which the data was recorded."
        }
      },
      {
        "name": "org_id",
        "type": "long",
        "metadata": {
          "comment": "The Internal ID for the customer's Samsara Org."
        }
      },
      {
        "name": "vg_device_id",
        "type": "long",
        "metadata": {
          "comment": "The ID of the vehicle gateway the Baxter device is connected to."
        }
      },
      {
        "name": "cm_device_id",
        "type": "long",
        "metadata": {
          "comment": "The ID of the camera the Baxter device is connected to."
        }
      },
      {
        "name": "date_baxter_first_connected",
        "type": "string",
        "metadata": {
          "comment": "Date Baxter device was first connected."
        }
      },
      {
        "name": "attribute_created_at_ts",
        "type": "timestamp",
        "metadata": {
          "comment": "Timestamp Baxter device attribute was created."
        }
      },
      {
        "name": "attribute_type",
        "type": "string",
        "metadata": {
          "comment": "Baxter device attribute type (Ignition On, Reverse Only)."
        }
      }
    ],
    "write_mode": "overwrite"
  }
}
