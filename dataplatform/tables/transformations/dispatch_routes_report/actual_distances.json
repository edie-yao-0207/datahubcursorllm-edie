{
  "name": "dispatch_routes_report.actual_distances",
  "owner": "Routing",
  "description": "A list of actual distances per route for planned versus actual reports, calculated using location ks. Each route will have a distance value per vehicle assignment.",
  "dependencies": [
    {
      "dbname": "dispatch_routes_report",
      "tablename": "route_vehicle_assignments",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "kinesisstats",
      "tablename": "location",
      "type": "table"
    }
  ],
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "dispatch_routes_report",
    "tablename": "actual_distances",
    "type": "table",
    "partition": ["route_scheduled_or_updated_date"],
    "primary_key": ["route_scheduled_or_updated_date", "org_id", "route_id", "vehicle_id", "scheduled_start_ms"],
    "schema": [
      {
        "name": "route_id",
        "type": "long"
      },
      {
        "name": "org_id",
        "type": "long"
      },
      {
        "name": "vehicle_id",
        "type": "long"
      },
      {
        "name": "actual_meters",
        "type": "double"
      },
      {
        "name": "scheduled_start_ms",
        "type": "long"
      },
      {
        "name": "route_scheduled_or_updated_date",
        "type": "date"
      }
    ],
    "write_mode": "merge"
  },
  "nonproduction": true,
  "disabled_regions": ["us-west-2", "eu-west-1", "ca-central-1"]
}
