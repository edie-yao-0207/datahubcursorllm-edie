{
  "name": "canonical_distance.total_distance",
  "owner": "TelematicsData",
  "description": "Compute the final distance deltas",
  "dependencies": [
    { "dbname": "productsdb", "tablename": "devices", "type": "table", "external": true },
    { "dbname": "definitions", "tablename": "canonical_distance_excluded_mmys", "type": "table", "external": true },
    { "dbname": "canonical_distance", "tablename": "filtered_odometer", "type": "table" },
    { "dbname": "canonical_distance", "tablename": "interpolated_gps", "type": "table" }
  ],
  "spark_execution_environment": {
    "spark_conf_overrides": {
      "spark.network.timeout": "600s"
    }
  },
  "parameters": ["start_date", "end_date", "pipeline_execution_time"],
  "backfill_start_date": "2021-01-31",
  "output": {
    "dbname": "canonical_distance",
    "tablename": "total_distance",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["date", "org_id", "device_id", "time_ms"],
    "schema": [
      { "name": "org_id", "type": "long" },
      { "name": "date", "type": "string" },
      { "name": "device_id", "type": "long" },
      { "name": "time_ms", "type": "long" },
      { "name": "distance", "type": "double" },
      { "name": "interpolated", "type": "integer", "nullable": false },
      { "name": "osd_odometer", "type": "long" }
    ],
    "write_mode": "merge"
  }
}
