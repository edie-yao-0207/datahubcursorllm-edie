{
  "name": "safety_report.trip_speeding",
  "owner": "SafetyScoringInsights",
  "nonproduction": false,
  "description": "Trip speeding duration and count",
  "dependencies": [
    {
      "external": true,
      "dbname": "trips2db_shards",
      "tablename": "trips",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "productsdb",
      "tablename": "devices",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "clouddb",
      "tablename": "organizations",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "clouddb",
      "tablename": "groups",
      "type": "table"
    }
  ],
  "backfill_start_date": "2018-01-01",
  "parameters": ["start_date", "end_date"],
  "unsharded_backfills": true,
  "output": {
    "dbname": "safety_report",
    "tablename": "trip_speeding",
    "type": "table",
    "partition": ["date"],
    "write_mode": "overwrite",
    "primary_key": ["org_id", "device_id", "start_ms", "version"],
    "schema": [
      {
        "name": "org_id",
        "type": "long",
        "metadata": {
          "comment": "$org_id_default_description"
        }
      },
      {
        "name": "device_id",
        "nullable": true,
        "type": "long",
        "metadata": {
          "comment": "The device ID of the trip."
        }
      },
      {
        "name": "driver_id",
        "type": "long",
        "metadata": {
          "comment": "The driver ID of the trip."
        }
      },
      {
        "name": "start_ms",
        "type": "long",
        "metadata": {
          "comment": "The start time in milliseconds of the trip."
        }
      },
      {
        "name": "end_ms",
        "type": "long",
        "metadata": {
          "comment": "The end time in milliseconds of the trip."
        }
      },
      {
        "name": "date",
        "type": "date"
      },
      {
        "name": "version",
        "type": "integer"
      },
      {
        "name": "not_speeding_count",
        "nullable": false,
        "type": "long"
      },
      {
        "name": "light_speeding_count",
        "nullable": false,
        "type": "long"
      },
      {
        "name": "moderate_speeding_count",
        "nullable": false,
        "type": "long"
      },
      {
        "name": "heavy_speeding_count",
        "nullable": false,
        "type": "long"
      },
      {
        "name": "severe_speeding_count",
        "nullable": false,
        "type": "long"
      },
      {
        "name": "not_speeding_ms",
        "nullable": false,
        "type": "long"
      },
      {
        "name": "light_speeding_ms",
        "nullable": false,
        "type": "long"
      },
      {
        "name": "moderate_speeding_ms",
        "nullable": false,
        "type": "long"
      },
      {
        "name": "heavy_speeding_ms",
        "nullable": false,
        "type": "long"
      },
      {
        "name": "severe_speeding_ms",
        "nullable": false,
        "type": "long"
      },
      {
        "name": "engine_idle_ms",
        "nullable": false,
        "type": "long"
      },
      {
        "name": "distance_meters",
        "nullable": false,
        "type": "double"
      },
      {
        "name": "driving_time_ms",
        "type": "long"
      },
      {
        "name": "seatbelt_reporting_present_ms",
        "type": "long"
      },
      {
        "name": "seatbelt_unbuckled_ms",
        "nullable": false,
        "type": "long"
      },
      {
        "name": "has_outward_camera",
        "type": "byte",
        "nullable": false
      },
      {
        "name": "has_inward_outward_camera",
        "type": "byte",
        "nullable": false
      }
    ]
  },
  "expectation": {
    "batch_mode": "FAIL_ALL"
  }
}
