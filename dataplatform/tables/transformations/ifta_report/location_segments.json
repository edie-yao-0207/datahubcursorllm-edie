{
  "name": "ifta_report.location_segments",
  "owner": "IftaCore",
  "description": "Builds initial location segments split on toll, jurisdiction, hour, or 15 minute duration.",
  "dependencies": [
    {
      "external": true,
      "dbname": "helpers",
      "tablename": "ignored_org_ids",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "kinesisstats",
      "tablename": "location",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "kinesisstats",
      "tablename": "osdlocationwithroadcontext",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "productsdb",
      "tablename": "devices",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "definitions",
      "tablename": "products",
      "type": "table"
    },
    {
      "dbname": "ifta_report",
      "tablename": "operating_authority_updates",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "clouddb",
      "tablename": "groups",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "fuel_energy_efficiency_report",
      "tablename": "driver_assignments",
      "type": "table"
    }
  ],
  "spark_execution_environment": {
    "spark_conf_overrides": {
      "spark.network.timeout": "600s"
    }
  },
  "backfill_start_date": "2023-03-31",
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "ifta_report",
    "tablename": "location_segments",
    "type": "table",
    "partition": ["date"],
    "write_mode": "overwrite",
    "primary_key": ["date", "org_id", "device_id", "start_ms"],
    "schema": [
      {
        "name": "date",
        "type": "date",
        "nullable": true,
        "metadata": {
          "comment": "The date associated with `start_ms`."
        }
      },
      {
        "name": "org_id",
        "type": "long",
        "nullable": true,
        "metadata": {
          "comment": "$org_id_default_description"
        }
      },
      {
        "name": "device_id",
        "type": "long",
        "nullable": true,
        "metadata": {
          "comment": "The device ID of the vehicle driven during this location segment."
        }
      },
      {
        "name": "start_ms",
        "type": "long",
        "nullable": true,
        "metadata": {
          "comment": "The start time in milliseconds of this location segment."
        }
      },
      {
        "name": "end_ms",
        "type": "long",
        "nullable": true,
        "metadata": {
          "comment": "The end time in milliseconds of this location segment."
        }
      },
      {
        "name": "value",
        "metadata": {
          "comment": "The location data that occurred at `start_ms`. Extends the `value` proto field from `kinesisstats.location`."
        },
        "type": {
          "type": "struct",
          "fields": [
            {
              "name": "time",
              "type": "long",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "has_fix",
              "type": "boolean",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "latitude",
              "type": "double",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "longitude",
              "type": "double",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "gps_speed_meters_per_second",
              "type": "double",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "heading_degrees",
              "type": "double",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "has_revgeo",
              "type": "boolean",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "revgeo_city",
              "type": "string",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "revgeo_state",
              "type": "string",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "revgeo_country",
              "type": "string",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "has_speed_limit",
              "type": "boolean",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "speed_limit_meters_per_second",
              "type": "double",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "has_toll",
              "type": "boolean",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "toll",
              "type": "boolean",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "off_highway",
              "type": "boolean",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "private_road",
              "type": "boolean",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "jurisdiction",
              "type": "string",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "operating_authority",
              "type": "integer",
              "nullable": true,
              "metadata": {}
            },
            {
              "name": "driver_id",
              "type": "long",
              "nullable": true,
              "metadata": {}
            }
          ]
        },
        "nullable": true
      }
    ]
  }
}
