{
  "name": "ifta_report.operating_authority_updates",
  "owner": "IftaCore",
  "description": "Extracts operating authority and driver ID from driver HOS logs.",
  "dependencies": [
    {
      "external": true,
      "dbname": "helpers",
      "tablename": "ignored_org_ids",
      "type": "table"
    },
    {
      "dbname": "compliancedb_shards",
      "tablename": "driver_carrier_entries",
      "type": "table",
      "external": true
    },
    {
      "dbname": "compliancedb_shards",
      "tablename": "driver_hos_vehicle_assignments",
      "type": "table",
      "external": true
    }
  ],
  "backfill_start_date": "2021-06-01",
  "parameters": ["start_date", "end_date", "pipeline_execution_time"],
  "output": {
    "dbname": "ifta_report",
    "tablename": "operating_authority_updates",
    "type": "table",
    "write_mode": "merge",
    "primary_key": ["date", "org_id", "vehicle_id", "log_at"],
    "schema": [
      {
        "name": "date",
        "type": "date",
        "metadata": {
          "comment": "The date of the HOS log's `log_at` field."
        }
      },
      {
        "name": "org_id",
        "type": "long",
        "metadata": {
          "comment": "$org_id_default_description"
        }
      },
      {
        "name": "vehicle_id",
        "type": "long",
        "nullable": false,
        "metadata": {
          "comment": "The ID of the vehicle that was assigned to the driver during this HOS log event."
        }
      },
      {
        "name": "log_at",
        "type": "timestamp",
        "metadata": {
          "comment": "The timestamp of the HOS log event."
        }
      },
      {
        "name": "driver_id",
        "type": "long",
        "metadata": {}
      },
      {
        "name": "operating_authority",
        "type": "integer",
        "metadata": {}
      },
      {
        "name": "updated_at",
        "type": "timestamp",
        "nullable": false,
        "metadata": {
          "comment": "The timestamp that this record was created or last updated. This mostly available for debugging."
        }
      }
    ]
  }
}
