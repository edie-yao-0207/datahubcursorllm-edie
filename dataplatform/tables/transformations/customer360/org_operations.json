{
  "name": "customer360.org_operations",
  "owner": "DataEngineering",
  "nonproduction": true,
  "description": "Create a row for each date customer first had a heartbeat to the current date",
  "spark_execution_environment": {
    "spark_version": "13.3.x-scala2.12",
    "spark_conf_overrides": {
      "spark.network.timeout": "600s",
      "spark.shuffle.partitions": "500",
      "spark.speculation": "true"
    }
  },
  "dependencies": [
    {
      "dbname": "customer360",
      "tablename": "org_dates",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "dataprep_cellular",
      "tablename": "att_daily_usage",
      "schema_path": "dataprep/cellular/jasper_rate_plans.metadata.json",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "clouddb",
      "tablename": "org_sfdc_accounts",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "clouddb",
      "tablename": "sfdc_accounts",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "trips2db_shards",
      "tablename": "trips",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "safetydb_shards",
      "tablename": "safety_events",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "safetydb_shards",
      "tablename": "safety_event_metadata",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "definitions",
      "tablename": "harsh_accel_type_enums",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "definitions",
      "tablename": "coaching_state_enums",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "productsdb",
      "tablename": "devices",
      "type": "table"
    },
    {
      "dbname": "dataprep",
      "tablename": "active_devices",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "customer360",
      "tablename": "customer_harsh_events",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "clouddb",
      "tablename": "historical_video_requests",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "clouddb",
      "tablename": "groups",
      "type": "table"
    }
  ],
  "backfill_start_date": "2015-06-19",
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "customer360",
    "tablename": "org_operations",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["date", "org_id"],
    "schema": [
      {
        "name": "org_id",
        "type": "long",
        "nullable": true
      },
      {
        "name": "sam_number",
        "type": "string",
        "nullable": true
      },
      {
        "name": "date",
        "type": "date",
        "nullable": true
      },
      {
        "name": "num_trips",
        "type": "long",
        "nullable": true
      },
      {
        "name": "total_trip_duration",
        "type": "long",
        "nullable": true
      },
      {
        "name": "total_trip_distance_meters",
        "type": "double",
        "nullable": true
      },
      {
        "name": "total_fuel_consumed_ml",
        "type": "double",
        "nullable": true
      },
      {
        "name": "total_not_speeding_ms",
        "type": "long",
        "nullable": true
      },
      {
        "name": "total_light_speeding_ms",
        "type": "long",
        "nullable": true
      },
      {
        "name": "total_moderate_speeding_ms",
        "type": "long",
        "nullable": true
      },
      {
        "name": "total_heavy_speeding_ms",
        "type": "long",
        "nullable": true
      },
      {
        "name": "total_severe_speeding_ms",
        "type": "long",
        "nullable": true
      },
      {
        "name": "total_harsh_events",
        "type": "long",
        "nullable": true
      },
      {
        "name": "total_data_usage",
        "type": "double",
        "nullable": true
      },
      {
        "name": "num_active_vg",
        "type": "long",
        "nullable": true
      },
      {
        "name": "num_active_cm",
        "type": "long",
        "nullable": true
      },
      {
        "name": "num_active_ag",
        "type": "long",
        "nullable": true
      },
      {
        "name": "num_active_ig",
        "type": "long",
        "nullable": true
      },
      {
        "name": "haBraking",
        "type": "long",
        "nullable": true
      },
      {
        "name": "haAccel",
        "type": "long",
        "nullable": true
      },
      {
        "name": "haSharpTurn",
        "type": "long",
        "nullable": true
      },
      {
        "name": "haCrash",
        "type": "long",
        "nullable": true
      },
      {
        "name": "haDistractedDriving",
        "type": "long",
        "nullable": true
      },
      {
        "name": "haTailgating",
        "type": "long",
        "nullable": true
      },
      {
        "name": "needs_review",
        "type": "long",
        "nullable": true
      },
      {
        "name": "needs_coaching",
        "type": "long",
        "nullable": true
      },
      {
        "name": "needs_recognition",
        "type": "long",
        "nullable": true
      },
      {
        "name": "reviewed",
        "type": "long",
        "nullable": true
      },
      {
        "name": "coached",
        "type": "long",
        "nullable": true
      },
      {
        "name": "recognized",
        "type": "long",
        "nullable": true
      },
      {
        "name": "dismissed",
        "type": "long",
        "nullable": true
      },
      {
        "name": "num_videos_retrieved",
        "type": "long",
        "nullable": true,
        "metadata": {
          "comment": "The number of videos retrieved by the customer on the specified date."
        }
      }
    ],
    "write_mode": "merge"
  },
  "replication": {
    "replicate_to_us_from_regions": ["eu-west-1"]
  }
}
