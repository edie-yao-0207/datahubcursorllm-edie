{
  "name": "customer360.org_compliance",
  "owner": "DataEngineering",
  "nonproduction": true,
  "description": "Determine the number of unassigned segments over time per Customer",
  "dependencies": [
    {
      "dbname": "compliancedb_shards",
      "tablename": "driver_hos_logs",
      "type": "table",
      "external": true
    },
    {
      "dbname": "clouddb",
      "tablename": "org_sfdc_accounts",
      "type": "table",
      "external": true
    },
    {
      "dbname": "clouddb",
      "tablename": "sfdc_accounts",
      "type": "table",
      "external": true
    }
  ],
  "backfill_start_date": "2015-06-19",
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "customer360",
    "tablename": "org_compliance",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["date", "org_id"],
    "schema": [
      {
        "name": "date",
        "type": "date",
        "nullable": true
      },
      {
        "name": "org_id",
        "type": "long"
      },
      {
        "name": "sam_number",
        "type": "string"
      },
      {
        "name": "num_unassigned_segments",
        "type": "long"
      },
      {
        "name": "unassigned_driver_time_hours",
        "type": "double"
      }
    ],
    "write_mode": "merge"
  },
  "replication": {
    "replicate_to_us_from_regions": ["eu-west-1"]
  }
}
