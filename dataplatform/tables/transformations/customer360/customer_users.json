{
  "name": "customer360.customer_users",
  "owner": "DataEngineering",
  "nonproduction": true,
  "description": "Create a row for each user per day for each customer and the associate number of page loads they made during that day.",
  "disabled_regions": ["eu-west-1", "ca-central-1"],
  "dependencies": [
    {
      "external": true,
      "dbname": "datamodel_platform_silver",
      "tablename": "stg_cloud_routes",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "clouddb",
      "tablename": "org_sfdc_accounts",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "clouddb",
      "tablename": "sfdc_accounts",
      "type": "table"
    }
  ],
  "backfill_start_date": "2015-06-19",
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "customer360",
    "tablename": "customer_users",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["date", "org_id", "sam_number", "user_email"],
    "schema": [
      {
        "name": "org_id",
        "type": "long",
        "metadata": {
          "comment": "This ID of customer's Samsara Org"
        }
      },
      {
        "name": "sam_number",
        "type": "string",
        "metadata": {
          "comment": "The SAM Number (internal Samsara ID) of a Customer"
        }
      },
      {
        "name": "date",
        "type": "date"
      },
      {
        "name": "user_email",
        "type": "string",
        "metadata": {
          "comment": "The user email of the account that was active on the dashboard"
        }
      },
      {
        "name": "num_page_loads",
        "type": "long",
        "metadata": {
          "comment": "The number of page loads that user made on that day"
        },
        "nullable": false
      }
    ],
    "write_mode": "merge"
  }
}
