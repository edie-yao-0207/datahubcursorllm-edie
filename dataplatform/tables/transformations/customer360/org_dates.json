{
  "name": "customer360.org_dates",
  "owner": "DataEngineering",
  "nonproduction": true,
  "description": "Create a row for each date customer first had a heartbeat to the current date",
  "dependencies": [
    {
      "external": true,
      "dbname": "dataprep",
      "tablename": "device_heartbeats_extended",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "dataprep",
      "tablename": "customer_metadata",
      "schema_path": "dataprep/business/customer_metadata.metadata.json",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "clouddb",
      "tablename": "organizations",
      "type": "table"
    }
  ],
  "backfill_start_date": "2015-06-19",
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "customer360",
    "tablename": "org_dates",
    "type": "table",
    "partition": ["date"],
    "primary_key": ["date", "org_id"],
    "schema": [
      {
        "name": "org_id",
        "type": "long"
      },
      {
        "name": "sam_number",
        "type": "string"
      },
      {
        "name": "first_heartbeat_date",
        "type": "string"
      },
      {
        "name": "date",
        "type": "date",
        "nullable": false
      }
    ],
    "write_mode": "merge"
  },
  "replication": {
    "replicate_to_us_from_regions": ["eu-west-1"]
  }
}
