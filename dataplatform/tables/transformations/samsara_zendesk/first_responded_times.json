{
  "name": "samsara_zendesk.first_responded_times",
  "owner": "DataEngineering",
  "nonproduction": true,
  "description": "Get the first responded times in calendar minutes for zendesk tickets",
  "dependencies": [
    {
      "external": true,
      "dbname": "samsara_zendesk",
      "tablename": "ticket_comment",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "samsara_zendesk",
      "tablename": "user",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "samsara_zendesk",
      "tablename": "ticket",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "samsara_zendesk",
      "tablename": "ticket_field_history",
      "type": "table"
    }
  ],
  "backfill_start_date": "2015-07-28",
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "samsara_zendesk",
    "tablename": "first_responded_times",
    "type": "table",
    "primary_key": ["ticket_id"],
    "schema": [
      {
        "name": "ticket_id",
        "type": "long",
        "metadata": {
          "comment": "The unique id of the ticket submitted."
        }
      },
      {
        "name": "last_updated_at",
        "type": "date",
        "metadata": {
          "comment": "The date of the last time the ticket had been updated."
        }
      },
      {
        "name": "first_responded_time_in_calendar_minutes",
        "type": "double",
        "metadata": {
          "comment": "How long it took for a representative to reply for the first time to the submitted ticket in minutes."
        },
        "nullable": false
      }
    ],
    "write_mode": "merge"
  },
  "disabled_regions": ["eu-west-1", "ca-central-1"]
}
