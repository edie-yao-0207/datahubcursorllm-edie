{
  "name": "samsara_zendesk.full_resolution_times",
  "owner": "DataEngineering",
  "nonproduction": true,
  "description": "Get the full resolution times in calendar minutes for zendesk tickets",
  "dependencies": [
    {
      "external": true,
      "dbname": "samsara_zendesk",
      "tablename": "ticket_field_history",
      "type": "table"
    },
    {
      "external": true,
      "dbname": "samsara_zendesk",
      "tablename": "ticket",
      "type": "table"
    }
  ],
  "backfill_start_date": "2015-07-28",
  "parameters": ["start_date", "end_date"],
  "output": {
    "dbname": "samsara_zendesk",
    "tablename": "full_resolution_times",
    "type": "table",
    "primary_key": ["ticket_id"],
    "schema": [
      {
        "name": "ticket_id",
        "type": "long",
        "metadata": {
          "comment": "The ticket id of the Zendesk support ticket submitted."
        }
      },
      {
        "name": "last_updated_at",
        "type": "date",
        "metadata": {
          "comment": "The data of when the ticket's status was last updated in Zendesk in YYYY-MM-DD format."
        }
      },
      {
        "name": "full_resolution_time_in_calendar_minutes",
        "type": "double",
        "nullable": false,
        "metadata": {
          "comment": "The time between when the ticket was created to when it was last marked as solved in calendar minutes."
        }
      }
    ],
    "write_mode": "merge"
  },
  "disabled_regions": ["eu-west-1", "ca-central-1"]
}
