{
  "table": "protocol_to_signal_settings",
  "owner": "FirmwareVdp",
  "description": "This table contains a mapping of each diagnostic protocols parsing rules. For instance, this contains parsing rules for J1939 SPNs and J1979 PIDs",
  "database": "definitions",
  "datatype": "csv",
  "schema": [
    {
      "name": "protocol",
      "type": "string"
    },
    {
      "name": "request_id",
      "type": "bigint"
    },
    {
      "name": "response_id",
      "type": "bigint"
    },
    {
      "name": "data_identifier",
      "type": "bigint"
    },
    {
      "name": "read_period_ms",
      "type": "bigint"
    },
    {
      "name": "signal_type",
      "type": "string"
    },
    {
      "name": "other_identifier",
      "type": "bigint"
    },
    {
      "name": "start_bit",
      "type": "bigint"
    },
    {
      "name": "bit_length",
      "type": "bigint"
    },
    {
      "name": "value_sign",
      "type": "string"
    },
    {
      "name": "factor",
      "type": "double"
    },
    {
      "name": "offset",
      "type": "double"
    },
    {
      "name": "minimum",
      "type": "double"
    },
    {
      "name": "maximum",
      "type": "double"
    },
    {
      "name": "unit",
      "type": "string"
    },
    {
      "name": "receiving_node",
      "type": "string"
    },
    {
      "name": "unit_conversion_scale",
      "type": "string"
    }
  ]
}
