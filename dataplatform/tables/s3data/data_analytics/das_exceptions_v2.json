{
  "table": "das_exceptions_v2",
  "owner": "DataAnalytics",
  "description": "Table that includes make model year engine type combos that should be excluded from swap action logic. This is currently pulling from a google sheet that our Product Support Engineering team owns. This table is referenced in the Diagnostics Available Service (DAS) pipeline.",
  "database": "data_analytics",
  "datatype": "csv",
  "schema": [
    {
      "name": "make",
      "type": "string",
      "metadata": {
        "comment": "Vehicle make."
      }
    },
    {
      "name": "model",
      "type": "string",
      "metadata": {
        "comment": "Vehicle model."
      }
    },
    {
      "name": "year",
      "type": "bigint",
      "metadata": {
        "comment": "Vehicle year."
      }
    },
    {
      "name": "engine_model",
      "type": "string",
      "metadata": {
        "comment": "Vehicle engine model."
      }
    },
    {
      "name": "product_type",
      "type": "string",
      "metadata": {
        "comment": "Product type."
      }
    },
    {
      "name": "cable_sku",
      "type": "string",
      "metadata": {
        "comment": "The cable sku."
      }
    }
  ]
}
